    function getFilteredData(id, type, Action) {
        idCopy = id;
        var ApprovalStageListCopy = [];
        let DocNum = '';
        if (type == 'DOCNUM') {

            if (Action == 'next') {
                let l1 = ApprovalStageList.map(m=> { return m.DOCNUM });
                l1 = l1.sort();
                var unique = l1.filter((v, i, a) => a.indexOf(v) === i);

                FilteredRecord = ApprovalStageList.filter(x=>x.DOCNUM == unique[0]);
                FilteredRecord.forEach(function (obj) {
                    Table = obj.Table;
                })

                if ($("#docNumber").val() == "") {
                    DocNum = unique[0];
                    id = DocNum;
                }
                else {
                    DocNum = $("#docNumber").val().substr(4, $("#docNumber").val().length);
                    DocNum = parseInt(DocNum) + 1;
                    if (DocNum > unique.length) {
                        return;
                    }
                    DocNum = DocNum.toString();
                    DocNum = 'Doc-' + DocNum.padStart(5, '0');
                    FilteredRecord = ApprovalStageList.filter(x=>x.DOCNUM == DocNum);
                    FilteredRecord.forEach(function (obj) {
                        Table = obj.Table;
                    })
                    id = DocNum;
                }
            }
            else if (Action == 'previous') {

                let l1 = ApprovalStageList.map(m=> { return m.DOCNUM });
                l1 = l1.sort();
                var unique = l1.filter((v, i, a) => a.indexOf(v) === i);

                FilteredRecord = ApprovalStageList.filter(x=>x.DOCNUM == unique[unique.length - 1]);
                FilteredRecord.forEach(function (obj) {
                    Table = obj.Table;
                })
                if ($("#docNumber").val() == "") {
                    DocNum = unique[unique.length - 1];
                    id = DocNum;
                }
                else {
                    DocNum = $("#docNumber").val().substr(4, $("#docNumber").val().length);
                    DocNum = parseInt(DocNum) - 1;
                    if (DocNum == 0) {
                        return;
                    }
                    DocNum = DocNum.toString();
                    DocNum = 'Doc-' + DocNum.padStart(5, '0');
                    FilteredRecord = ApprovalStageList.filter(x=>x.DOCNUM == DocNum);
                    FilteredRecord.forEach(function (obj) {
                        Table = obj.Table;
                    })
                    id = DocNum;
                }
            }

            else if (Action == 'last') {
                let l1 = ApprovalStageList.map(m=> { return m.DOCNUM });
                l1 = l1.sort();
                var unique = l1.filter((v, i, a) => a.indexOf(v) === i);

                id = ApprovalStageList.filter(x=>x.DOCNUM == unique[unique.length - 1]);
                if (id.length > 0) {
                    id = id[0].DOCNUM;
                    FilteredRecord = ApprovalStageList.filter(x=>x.DOCNUM == id);
                    FilteredRecord.forEach(function (obj) {
                        Table = obj.Table;
                    })
                }
            }

            else if (Action == 'first') {
                id = ApprovalStageList[0].DOCNUM;
                FilteredRecord = ApprovalStageList.filter(x=>x.DOCNUM == id);
                FilteredRecord.forEach(function (obj) {
                    Table = obj.Table;
                })
                if (id == "") {
                    console.log("null id");
                }
            }
        }

        var ASC;
        var ASD;
        var ASAR;
        var ASRR;
        var AA;
        FilteredRecord.forEach(function (obj) {
            AA = obj.ID;
            ASC = obj.STAGECODE;
            ASD = obj.STAGEDESCRIPTION;
            ASAR = obj.APPROVALREQUIRED;
