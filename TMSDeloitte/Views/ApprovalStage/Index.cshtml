        $('#add_update').click(function () {
            var isSuccess = true;
            var message = '';
            var alertSetupObj = {
                StageCode: $("#StageCode").val(),
                StageDescription: $("#StageDescription").val(),
                ApprovalRequired: $("#ApprovalRequired").val(),
                RejectionRequired: $("#RejectionRequired").val(),
            };
            if (alertSetupObj.StageCode == "") {
                isSuccess = false;
                message = 'Please enter stage code. \n';
            }
            if (alertSetupObj.StageDescription == "") {
                isSuccess = false;
                message = message + 'Please enter stage description. \n';
            }
            if (alertSetupObj.ApprovalRequired == "") {
                isSuccess = false;
                message = message + 'Please select approval required. \n';
            }
            if (alertSetupObj.RejectionRequired == "") {
                isSuccess = false;
                message = message + 'Please enter rejection required. \n';
            }
            var approvalreq = $("#ApprovalRequired").val()
            var rejectionreq = $("#RejectionRequired").val()
            if (approvalreq > Table.length || rejectionreq > Table.length) {
                isSuccess = false;
                message = "Please decrease value of approval or rejection required to add/update otherwise add another approver"
            }
            if (isSuccess) {
                var gridContainer = $("#gridContainer").dxDataGrid("instance");
                loadPanel.show();
                gridContainer.getController('validating').validate(true).done(function (result) {
                    if (result) {
                        SaveEditedData = gridContainer.saveEditData();
                        setTimeout(function () { AddUpdate(); }, 2000);
                    }
                    else {
                        loadPanel.hide();
                        AlertToast('error', "Please fill required fields!");
                    }

                });
            }
            else {
                AlertToast('error', message);
            }
        });


