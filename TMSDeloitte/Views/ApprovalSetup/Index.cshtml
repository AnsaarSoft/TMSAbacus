    function getFilteredData(id, type, Action) {
        idCopy = id;
        var ApprovalSetupListCopy = [];
        let DocNum = '';
        if (type == 'DOCNUM') {

            if (Action == 'next') {
                let l1 = ApprovalSetupList.map(m=> { return m.DOCNUM });
                l1 = l1.sort();
                var unique = l1.filter((v, i, a) => a.indexOf(v) === i);

                FilteredRecord = ApprovalSetupList.filter(x=>x.DOCNUM == unique[0]);
                FilteredRecord.forEach(function (obj) {
                    Table = obj.Table;
                })

                if ($("#docNumber").val() == "") {
                    DocNum = unique[0];
                    id = DocNum;
                }
                else {
                    DocNum = $("#docNumber").val().substr(4, $("#docNumber").val().length);
                    DocNum = parseInt(DocNum) + 1;
                    if (DocNum > unique.length) {
                        return;
                    }
                    DocNum = DocNum.toString();
                    DocNum = 'Doc-' + DocNum.padStart(5, '0');
                    FilteredRecord = ApprovalSetupList.filter(x=>x.DOCNUM == DocNum);
                    FilteredRecord.forEach(function (obj) {
                        Table = obj.Table;
                    })
                    id = DocNum;
                }
            }
            else if (Action == 'previous') {

                let l1 = ApprovalSetupList.map(m=> { return m.DOCNUM });
                l1 = l1.sort();

                var unique = l1.filter((v, i, a) => a.indexOf(v) === i);

                FilteredRecord = ApprovalSetupList.filter(x=>x.DOCNUM == unique[unique.length - 1]);
                FilteredRecord.forEach(function (obj) {
                    Table = obj.Table;
                })
                if ($("#docNumber").val() == "") {
                    DocNum = unique[unique.length - 1];
                    id = DocNum;
                    //DocNum = ApprovalSetupList[0].DOCNUM;
                    //id = DocNum;
                }
                else {
                    DocNum = $("#docNumber").val().substr(4, $("#docNumber").val().length);
                    DocNum = parseInt(DocNum) - 1;
                    if (DocNum == 0) {
                        return;
                    }
                    DocNum = DocNum.toString();
                    DocNum = 'Doc-' + DocNum.padStart(5, '0');
                    FilteredRecord = ApprovalSetupList.filter(x=>x.DOCNUM == DocNum);
                    FilteredRecord.forEach(function (obj) {
                        Table = obj.Table;
                    })
                    id = DocNum;
                }
            }

            else if (Action == 'last') {
                let l1 = ApprovalSetupList.map(m=> { return m.DOCNUM });
                l1 = l1.sort();

                var unique = l1.filter((v, i, a) => a.indexOf(v) === i);

                id = ApprovalSetupList.filter(x=>x.DOCNUM == unique[unique.length - 1]);
                if (id.length > 0) {
                    id = id[0].DOCNUM;
                    FilteredRecord = ApprovalSetupList.filter(x=>x.DOCNUM == id);
                    FilteredRecord.forEach(function (obj) {
                        Table = obj.Table;
                    })
                }
            }

            else if (Action == 'first') {
                id = ApprovalSetupList[0].DOCNUM;
                FilteredRecord = ApprovalSetupList.filter(x=>x.DOCNUM == id);
                FilteredRecord.forEach(function (obj) {
                    Table = obj.Table;
                })
                if (id == "") {
                    console.log("null id");
                }
            }

            //Table = ApprovalSetupList.filter(x=>x.DOCNUM == id);
            //if (Table.length == 0) {
            //    Table = ApprovalSetupList.filter(x=>x.DOCNUM == idCopy);
            //}
        }

        var AC;
        var AN;
        var AS;
        var AD;
        var AA;
        FilteredRecord.forEach(function (obj) {
            AA= obj.ID;
            AC = obj.APPROVALCODE;
            AN = obj.APPROVALDESCRIPTION;
            AS = obj.APPROVALSTAGE;
            $("#ApprovalStage").dxSelectBox({ value: AS });
            AD = obj.DOCUMENT;
            GenerateGrid(obj.Table);
            Table = obj.Table;
        })

        if (Table.length > 0) {
            DocNum = Table[0].DOCNUM;
        }

        DocId = AA;
        $("#ApprovalCode").val(AC);
        $("#ApprovalDescription").val(AN);
        $("#Document").dxSelectBox({ value: AD });
        //$("#Document").val(AD);

        $('#docNumber').prop("disabled", true);
        $("#IsActive").dxCheckBox({
            value: FilteredRecord[0].ISACTIVE
        });
        if (id != undefined) {
            $("#docNumber").val(id);
        }
    }


