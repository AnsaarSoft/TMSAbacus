    function GetApproval_Setup() {
        var document = $("#Document").val();
        var url = "/ApprovalSetup/GetApprovalSetup";
        $.ajax({
            url: url,
            method: "GET",
            data: { APPROVALSTAGE: $("#ApprovalStage").val(), DOCUMENT: document },
            async: false,
        }).done(function (data) {
            AllEmployees = data.response;
            ApprovalSetupList = data.ApprovalSetup;
            var abc = [];
            GenerateGrid(abc);
            isPageDataLoaded = true;
        }).fail(function (data) {
            isPageDataLoaded = true;
            loadPanel.hide();
        });
    }

    function ValidateAuthorization() {
        var id = $("#AuthID").val();
        if (id != -1) {
            if (id == 2) {
                $("#add_update").remove();
                $("#gridContainer").dxDataGrid({ disabled: true });

            }
        }
    }

    function AddUpdate() {
        var notSaved = $('#gridContainer').dxDataGrid('instance').getController('editing').hasChanges();
        var ID = 0;
        if (notSaved)
            setTimeout(function () { AddUpdate(); }, 2000);

        else {
            if (Table.length > 0) {

                FilteredRecordForID = ApprovalSetupList.filter(x=>x.DOCNUM == $("#docNumber").val());
                if (FilteredRecordForID.length > 0) {
                    ID = FilteredRecordForID[0].ID;
                }

                var ISACTIVE = $("#IsActive").dxCheckBox("instance").option('value');
                loadPanel.show();
                var url = "/ApprovalSetup/AddUpdateApprovalSetup";
                $.ajax({
                    type: "POST",
                    url: url,
                    contentType: 'application/json',
                    data: JSON.stringify({

                        APPROVALCODE: $("#ApprovalCode").val(),
                        APPROVALDESCRIPTION: $("#ApprovalDescription").val(),
                        APPROVALSTAGE: $("#ApprovalStage").val(),
                        DOCUMENT: $("#Document").val(),
                        DOCNUM: $("#docNumber").val(),
                        ISACTIVE: ISACTIVE,
                        ApprovalSetupInfo: Table,
                        ID: ID
                    }),
                    dataType: "json",
                    async: false,
                    success: function (response) {
                        loadPanel.hide();
                        if (response.Success) {
                            AlertToast('success', response.Message);
                            setTimeout(function () {
                                window.location.reload();
                            }, 5000);
                        }
                        else
                            AlertToast('error', response.Message);
                    },
                    error: function (response) {
                        console.log(response);
                        loadPanel.hide();
                    },
                    failure: function (response) {
                        console.log(response);
                        loadPanel.hide();
                    }

                })
            }
            else {
                loadPanel.hide();
                AlertToast('error', "Please add atleast one new record");
            }
        }
    }

